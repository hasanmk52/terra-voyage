{
  "name": "terra-voyage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:vercel": "prisma generate && prisma migrate deploy && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "node -r ./scripts/load-env.js node_modules/.bin/prisma generate",
    "db:push": "node -r ./scripts/load-env.js node_modules/.bin/prisma db push",
    "db:seed": "node -r ./scripts/load-env.js node_modules/.bin/tsx prisma/seed.ts",
    "db:studio": "node -r ./scripts/load-env.js node_modules/.bin/prisma studio",
    "db:reset": "node -r ./scripts/load-env.js node_modules/.bin/prisma migrate reset",
    "db:test": "node -r ./scripts/load-env.js node_modules/.bin/tsx scripts/test-database.ts",
    "test:env": "node -r ./scripts/load-env.js node_modules/.bin/tsx scripts/test-complete-setup.ts",
    "test:redis": "node -r ./scripts/load-env.js node_modules/.bin/tsx scripts/test-redis-setup.ts",
    "test:validation": "node -r ./scripts/load-env.js node_modules/.bin/tsx scripts/test-date-overlap-validation.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
